{
    "compilerOptions": {
      "module": "ES2022",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "target": "ES2022",
      "moduleResolution": "node",
      "sourceMap": true,
      "outDir": "build",
      "baseUrl": ".",
      "paths": {
          "*": [
              "node_modules/*",
              "."
          ]
      }
    },
    "include": ["./**/*", "../interface/**/*"]
  }